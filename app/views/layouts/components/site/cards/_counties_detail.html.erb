<%= link_to url, class: "innerShadow hasWidth flex flex-col  m-4 rounded relative w-full lg:w-1/3 xl:w-1/3" do %>
  <%= image_tag url_for(image), class: "w-full h-48" if image.attached?  %>
  <div class="flex-col flex px-4 z-10 mt-4">
    <span class="z-10 font-normal text-lg"><%= county %></span>
  </div>  
  <div class="px-4 z-10 mb-4 mt-2">
    <span class="z-10 font-semibold text-lg text-center"><%= title %></span>
  </div>
<% end %>